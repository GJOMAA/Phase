; Test case for the 2D flow around box

CaseName FlowAroundBox

Solver
{
	timeDependent Off
	maxIterations 100
	timeStep 0.0015
	maxTime 15
	maxCo 0.8

	numInnerIterations 1
	numPressureCorrections 2
	momentumRelaxation 1
	pressureCorrectionRelaxation 0.5
}

LinearAlgebra
{
  uEqn
  {
    lib trilinos
    solver BiCGSTAB
    preconditioner RILUK
    iluFill 1
    maxIterations 50
    tolerance 1e-6
  }

  pEqn
  {
    lib trilinos
    solver BiCGSTAB
    preconditioner RILUK
    iluFill 1
    maxIterations 300
    tolerance 1e-6
  }
}

Properties
{
	rho 1
	mu 8.94e-4
	g (0,0)
	gamma 0.001
}

Grid
{
	type cgns
	filename "case/BoxMesh.cgns"
}

Viewer
{
	type tecplot
	vectorFields u,us
	scalarFields p,phi
}

Integrators
{
  ForceIntegrators
  {
    patches BoxWalls
  }
}

System
{
  fileWriteFrequency 50
}
