; Test case for the 2D flat plate

CaseName FlatPlate2D

Solver
{
	type "poisson"
	timeStep 0.01
	maxTime 30
}

LinearAlgebra
{
	phiEqn
	{
		lib eigen
	}
}

Properties
{
	gamma 1
}

Grid
{
	type rectilinear
	nCellsX 100
	nCellsY 100

	; Rectilinear parameters
	width 1
	height 1
}

Viewer
{
	type tecplot
	vectorFields
	scalarFields phi
}
