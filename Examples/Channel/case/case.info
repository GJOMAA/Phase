; Test case for the 2D Channel

CaseName Channel2D

Solver
{
	timeDependent Off
	maxIterations 100
	timeStep 5e-4
	maxTime 1
	maxCo 0.8

	numInnerIterations 1
	numPressureCorrections 2
	momentumRelaxation 0.5
	pressureCorrectionRelaxation 0.5
}

LinearAlgebra
{
  uEqn
  {
    lib trilinos
    solver BiCGSTAB
    iluFill 0
    tolerance 1e-14
  }

  pEqn
  {
    lib muelu
    solver GMRES
    iluFill 6
    tolerance 1e-14
    parameterFile mg.xml
  }
}

Properties
{
	rho 1.2
	mu 1e-2
	g (0,0)
	gamma 0.001
}

Grid
{
	type rectilinear
	nCellsX 300
	nCellsY 150

	; Rectilinear parameters
	width 0.8
	height 0.4
}

Viewer
{
	type tecplot
	integerFields proc
	vectorFields u,gradP
	scalarFields p,pCorr,m
}

System
{
  fileWriteFrequency 10
}
