; Test case for the 2D Channel

CaseName Channel2D

Solver
{
	timeDependent Off
	maxIterations 100
	timeStep 2.5e-3
	maxTime 20
	maxCo 1

	numInnerIterations 1
	numPressureCorrections 2
	momentumRelaxation 0.5
	pressureCorrectionRelaxation 0.5
	;symmetryAxis (0,1)
	axisPoint (-0.1,0)
}

LinearAlgebra
{
  uEqn
  {
    lib eigen
    solver BiCGSTAB
    iluFill 0
    tolerance 1e-14
  }

  pEqn
  {
    lib eigen
    solver klu2
  }
}

Properties
{
	rho 1
	mu 1e-3
	g (0,0)
	gamma 0.001
}

Grid
{
	type rectilinear
	nCellsX 200
	nCellsY 100

	; Rectilinear parameters
	width 0.8
	height 0.4
}

Viewer
{
	type tecplot
	integerFields proc
	vectorFields u,gradP
	scalarFields p,pCorr,m
}

System
{
  fileWriteFrequency 10
}
